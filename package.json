{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.6.0",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "// Tasks": "",
    "build": "tsx scripts/build.ts",
    "build-release": "tsx scripts/build.ts",
    "check": "tsc --noEmit",
    "format": "npm run dprint fmt",
    "format-check": "npm run dprint check && npm run ec",
    "link-check": "markdown-link-check README.md",
    "lint": "eslint -c .config/eslintrc.yml --ext .ts __tests__ src",
    "test": "jest -c .config/jest.config.mjs --coverage",
    "preversion": "npm run build-release",
    "version": "bash scripts/bump-version.sh",
    "// Commands:": "",
    "dprint": "dprint -c .config/dprint.json",
    "ec": "git ls-files -z | xargs -0 ec -exclude '^dist'"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "engines": {
    "node": ">=16"
  },
  "imports": {
    "##/*": "./*"
  },
  "dependencies": {
    "@actions/cache": "3.2.1",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.1.0",
    "@actions/io": "1.1.3",
    "@actions/tool-cache": "2.0.1",
    "class-transformer": "0.5.1",
    "decorator-cache-getter": "1.0.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "dprint": "0.35.4",
    "editorconfig-checker": "5.0.1",
    "es-main": "1.2.0",
    "esbuild": "0.17.16",
    "esbuild-loader": "3.0.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "46.0.0",
    "git-cliff": "1.1.2",
    "jest": "29.5.0",
    "jest-extended": "3.2.4",
    "license-webpack-plugin": "4.0.2",
    "markdown-link-check": "3.11.0",
    "ts-dedent": "2.2.0",
    "ts-essentials": "9.3.1",
    "ts-jest": "29.1.0",
    "tsconfig-paths-webpack-plugin": "4.0.1",
    "tsx": "3.12.6",
    "typescript": "5.0.4",
    "webpack": "5.79.0"
  },
  "overrides": {
    "eslint-plugin-import": {
      "tsconfig-paths": "^4.1.1"
    }
  }
}
