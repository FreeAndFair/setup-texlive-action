{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.3.1",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "ttsc",
    "postbuild": "node .config/esbuild.config.mjs lib/index.js",
    "format": "dprint -c .config/dprint.json fmt",
    "format-check": "dprint -c .config/dprint.json check && ec --exclude=^dist",
    "lint": "eslint -c .config/eslintrc.yml --ext .ts __tests__ src",
    "postinstall": "patch-package --patch-dir .config/patches",
    "test": "jest -c .config/jest.config.mjs --coverage",
    "type-check": "ttsc --noEmit",
    "version": "bash scripts/bump-version.sh"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@actions/cache": "3.0.4",
    "@actions/core": "1.9.1",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.3.0",
    "@actions/io": "1.1.2",
    "@actions/tool-cache": "2.0.1",
    "class-transformer": "0.5.1",
    "decorator-cache-getter": "1.0.0",
    "reflect-metadata": "0.1.13",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/node": "18.7.13",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "dprint": "0.30.3",
    "editorconfig-checker": "4.0.2",
    "esbuild": "0.15.5",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.7",
    "eslint-plugin-node": "11.1.0",
    "jest": "28.1.3",
    "jest-extended": "3.0.2",
    "jest-mock-extended": "2.0.7",
    "patch-package": "6.4.7",
    "ts-essentials": "9.3.0",
    "ts-jest": "28.0.8",
    "ts-transformer-keys": "0.4.3",
    "ttypescript": "1.5.13",
    "typescript": "4.7.4",
    "typescript-transform-paths": "3.3.1"
  }
}
